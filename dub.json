{
    "name": "ddata",
    "authors": [
        "Ali Akhtarzada"
    ],
    "description": "Data library for D",
    "copyright": "Copyright Â© 2017, Ali Akhtarzada",
    "license": "MIT",
    "targetPath": "bin",
    "dependencies": {
        "ddata:jwt": "*",
        "ddata:crypto": "*"
    },
    "subPackages": [
        "./jwt/",
        "./crypto/",
        "./common/"
    ],
    "configurations": [
        {
            "name": "unittest",
            "sourcePaths": [
                "./jwt/",
                "./crypto/",
                "./common/"
            ],
            "importPaths": [
                "."
            ]
        },
        {
            "name": "unittest-safe",
            "sourcePaths": [
                "./jwt/",
                "./crypto/",
                "./common/"
            ],
            "dflags": ["-dip1000"],
            "importPaths": [
                "."
            ]
        }
    ]
}
